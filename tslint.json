{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [true, "single"],
    "no-console": [true,
      "assert",
      "clear",
      "count",
      "dir",
      "dirxml",
      "debug",
      "error",
      "group",
      "groupCollapsed",
      "groupEnd",
      "info",
      "profile",
      "profileEnd",
      "table",
      "time",
      "timeEnd",
      "timeStamp",
      "trace",
      "warn"
    ],
     "no-reference": false
  }
}